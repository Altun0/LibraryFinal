<body>
    <h1>Kutubhane Girish Paneli</h1>
    <div class=" w3l-login-form">
        <h2>Login</h2>
        @using (Html.BeginForm("GirisYap", "Login", FormMethod.Post))
        {
            <div class=" w3l-form-group">
                <label>Kullanici Adi:</label>
                <div class="group">
                    <i class="fas fa-user"></i>
                    <input type="text" class="form-control" placeholder="kullanici adiniz" required="required" name="MAIL" />
                </div>
            </div>
            <div class=" w3l-form-group">
                <label>Sifre:</label>
                <div class="group">
                    <i class="fas fa-unlock"></i>
                    <input type="password" class="form-control" placeholder="sifreniz" required="required" name="SIFRE" />
                </div>
            </div>
            <div class="forgot">
                <a href="#">Sifremi Unutdum?</a>
                <p><input type="checkbox">Benim Hatirla</p>
            </div>
            <button type="submit">Girish yap</button>
        }
        <p class=" w3l-register-p">Bir Hesabiniz Yok mu?<a href="/KayitOl/Kayit/" class="register"> Kayit Ol</a></p>
    </div>
    <footer>
        <p class="copyright-agileinfo"> &copy; 2022 Tum haklari saklidir | design by <a href="http://w3layouts.com">W3layouts</a></p>
